  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "node-sass": "^4.11.0",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "watch": "^1.0.2"
  },
  "dependencies": {}
}
